<span class="threadVotes"><%= reply.votes %></span> - <span class="username"><%= reply.user %></span>
<p><%= reply.content %></p>
<% if (reply.replies && reply.replies.length) { %>
    <ul>
    <% reply.replies.forEach(function(reply) { %>
        <li>
            <%- include('../partials/reply', {reply: reply}); %>
        </li>
    <% }); %>
    </ul>
<% }; %>